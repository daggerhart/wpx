{
    "name": "daggerhartlab/wpx",
    "description": "WordPress library",
    "type": "library",
    "authors": [
        {
            "name": "Jonathan Daggerhart",
            "email": "jonathan@daggerhart.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.3",
        "php-di/php-di": "^6.3",
        "monolog/monolog": "^2.0",
        "hassankhan/config": "^3.0",
        "symfony/options-resolver": "^4.4 || ^5.4",
        "symfony/finder": "^4.4 || ^5.4",
        "symfony/yaml": "^4.4 || ^5.4"
    },
    "autoload": {
        "classmap": [
            "Wpx.php"
        ],
        "psr-4": {
            "Wpx\\": "src/"
        }
    },
    "require-dev": {
        "wp-cli/wp-cli": "^2.0"
    }
}
